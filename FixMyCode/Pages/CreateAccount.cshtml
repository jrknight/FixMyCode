@page
@model FixMyCode.Pages.CreateAccountModel
@{
    ViewData["Title"] = "CreateAccount";
}

<h2>CreateAccount</h2>

<html>

    <link rel = "stylesheet" href="site.css" />

    <div>
        <form>
            Full Name<br>
            <input type="text" name="fullname" class="name_entry" />
            Email<br>
            <input type="email" name="emailadd" class="email_entry" />
            Are you a student?<br>
            <input type="checkbox" class="yes_stbox" />
            <input type="submit" value="Submit" class="submit_create" />

        </form>
    </div>

</html>

